---
import { type CollectionEntry } from "astro:content";
import PostCard from "./PostCard.astro";

type PostEntry = CollectionEntry<"blog">;

interface Props {
  posts: PostEntry[];
}
---

<section>
  <ul>
    {
      Astro.props.posts.map((post) => (
        <li>
          <PostCard post={post} />
        </li>
      ))
    }
  </ul>
</section>
